    "url": "git+https://github.com/kubens/eco-calendar.git"
  },
  "dependencies": {
    "eco-scheduler": "^1.0.0",
    "ics-generator": "^0.1.0",
    "itty-router": "^3.0.12"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230404.0",
    "wrangler": "^2.15.1"
  }
}
